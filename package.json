{
    "name": "angular-starter-material-graphql-ngrx-store",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start:ng": "ng serve",
        "start": "run-p start:ng gql:codegen:watch",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "generate:introspect-schema": "apollo-codegen introspect-schema http://localhost:8080/graphql --output src/generated/schema.graphql",
        "generate:types": "apollo-codegen generate src/**/*.ts --schema src/generated/schema.json --target typescript --output src/generated/operation-result-types.ts",
        "dev:ssr": "ng run angular-starter-material-graphql-ngrx-store:serve-ssr",
        "serve:ssr": "node dist/angular-starter-material-graphql-ngrx-store/server/main.js",
        "build:ssr": "ng build --prod && ng run angular-starter-material-graphql-ngrx-store:server:production",
        "prerender": "ng run angular-starter-material-graphql-ngrx-store:prerender",
        "prebuild:ssr": "ngcc",
        "gql:codegen": "graphql-codegen --config codegen.yml",
        "gql:codegen:watch": "graphql-codegen --watch"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~9.1.4",
        "@angular/cdk": "^9.2.4",
        "@angular/common": "~9.1.4",
        "@angular/compiler": "~9.1.4",
        "@angular/core": "~9.1.4",
        "@angular/flex-layout": "^10.0.0-beta.32",
        "@angular/forms": "~9.1.4",
        "@angular/material": "^9.2.4",
        "@angular/platform-browser": "~9.1.4",
        "@angular/platform-browser-dynamic": "~9.1.4",
        "@angular/platform-server": "~9.1.4",
        "@angular/router": "~9.1.4",
        "@apollo/client": "^3.1.4",
        "@nestjs/common": "^6.0.0",
        "@nestjs/core": "^6.0.0",
        "@nestjs/ng-universal": "^3.2.0",
        "@nestjs/platform-express": "^6.0.0",
        "@ngrx/effects": "^10.0.0",
        "@ngrx/entity": "^10.0.0",
        "@ngrx/router-store": "^10.0.0",
        "@ngrx/store": "^10.0.0",
        "@ngrx/store-devtools": "^10.0.0",
        "@nguniversal/express-engine": "^9.1.1",
        "apollo-angular": "^2.0.3",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.9.1",
        "express": "^4.15.2",
        "graphql": "^15.0.0",
        "i": "^0.3.6",
        "npm": "^6.14.8",
        "reflect-metadata": "^0.1.13",
        "rxjs": "~6.5.4",
        "tslib": "^1.10.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.901.4",
        "@angular/cli": "~9.1.4",
        "@angular/compiler-cli": "~9.1.4",
        "@angular/language-service": "~9.1.4",
        "@graphql-codegen/cli": "1.17.8",
        "@graphql-codegen/typescript-apollo-angular": "^2.0.1",
        "@graphql-codegen/typescript-operations": "^1.17.8",
        "@nguniversal/builders": "^9.1.1",
        "@types/express": "^4.17.0",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "codelyzer": "^5.1.2",
        "graphql-cli": "^4.0.0",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-jasmine": "~3.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "ngrx-store-freeze": "^0.2.4",
        "npm-run-all": "^4.1.5",
        "protractor": "~5.4.3",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~3.8.3"
    }
}
